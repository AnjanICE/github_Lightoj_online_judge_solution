#include<stdio.h>
int main()
{
    int n,a=0,i=0,t,sum=0;
    scanf("%d",&t);
    while(t-- && scanf("%d",&n)==1){
    while(n!=0){
        a=n%2;
        n=n/2;
        sum=sum+a;
    }
    if(sum%2==0)
         printf("Case %d: even\n",++i);
    else
          printf("Case %d: odd\n",++i);
    sum=0;
    }
    return 0;
    }
